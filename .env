# 应用配置
APP_NAME=ai-travel-planner
APP_VERSION=1.0.0
ENV=development
DEBUG=true

# 数据库配置  
DATABASE_URL=mysql+aiomysql://root:password@localhost:3306/ai_travel_planner
DATABASE_POOL_SIZE=10
DATABASE_MAX_OVERFLOW=20
ENABLE_QUERY_LOG=true

# Redis配置
REDIS_URL=redis://localhost:6379/0
REDIS_SESSION_DB=1
REDIS_CACHE_DB=2

# 测试配置
TEST_DATABASE_URL=sqlite+aiosqlite:///test.db
